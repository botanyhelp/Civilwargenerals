<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jimsuplee.civilwargenerals" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/civilwar"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Civilwargenerals"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".General"
            android:label="General" >
            <intent-filter>
                <action android:name="com.jimsuplee.civilwargenerals.General" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pictures"
            android:label="Pictures" >
            <intent-filter>
                <action android:name="com.jimsuplee.civilwargenerals.Pictures" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
